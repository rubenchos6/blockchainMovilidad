{
  "name": "safemovapp",
  "version": "0.0.0",
  "description": "This box comes with everything you need to get started with HL Besu",
  "keywords": [
    "besu",
    "hyperledger",
    "nestjs",
    "truffle",
    "elastic"
  ],
  "author": "Giuseppe",
  "license": "MIT License",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "private": true,
  "scripts": {
    "ng": "ng",
    "a:start": "ng serve",
    "a:build": "ng build",
    "a:watch": "ng build --watch --configuration development",
    "a:test": "ng test",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "lite-server",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "besu:docker": "docker run -p 8545:8545 --mount type=bind,source=/tmp/besu/dev,target=/var/lib/besu hyperledger/besu:latest --miner-enabled --miner-coinbase 0x42db57dfc5ec008a8404f706601541ae19d2e51b --rpc-http-enabled --rpc-http-cors-origins=\"all\" --host-allowlist=\"*\" --network=dev --data-path=~/Besu/besu-22.1.1/Clique-Network/Node-1/data",
    "besu:alethio": "docker run --rm -p 25000:80 -e APP_NODE_URL=http://localhost:8545 alethio/ethereum-lite-explorer",
    "besu:test": "besu --config-file=./besu_conf/besu_test_conf.toml",
    "besu:ropsten": "besu  --network=ropsten --rpc-http-enabled --rpc-http-cors-origins 'http://remix.ethereum.org'",
    "besu:rinkeby": "besu --network=rinkeby --data-path=<path>/<rinkebydata-path>",
    "besu:goerli": "besu --network=goerli --data-path=<path>/<goerlidata-path>",
    "besu:main": "besu --rpc-http-enabled",
    "transfer": "node ./client_script/transfer.js"
  },
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/serve-static": "^2.1.0",
    "@openzeppelin/test-environment": "^0.1.9",
    "axios": "^0.21.1",
    "chai": "^4.3.6",
    "cjson": "^0.5.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "elastic-apm-node": "^3.31.0",
    "ethereumjs-tx": "^2.1.2",
    "helmet": "^3.22.0",
    "hex2dec": "^1.1.2",
    "mocha": "^9.2.2",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "truffle-contract": "^4.0.31",
    "tslib": "^2.3.0",
    "web3": "^1.2.7",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.3",
    "@angular/cli": "~13.3.3",
    "@angular/compiler-cli": "~13.3.0",
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@truffle/hdwallet-provider": "^1.0.34",
    "@types/express": "^4.17.3",
    "@types/jasmine": "~3.10.0",
    "@types/jest": "25.1.4",
    "@types/node": "^13.9.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jasmine-core": "~4.0.0",
    "jest": "^25.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lite-server": "^2.6.1",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "truffle": "^5.1.22",
    "ts-jest": "25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
